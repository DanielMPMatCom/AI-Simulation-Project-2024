<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/electricity_ontology.owl"
         xmlns="http://example.org/electricity_ontology.owl#">

<owl:Ontology rdf:about="http://example.org/electricity_ontology.owl"/>

<owl:ObjectProperty rdf:about="#affects">
  <rdfs:domain rdf:resource="#Circuit"/>
  <rdfs:range rdf:resource="#Citizen"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasEconomicImpact">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Circuit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasAffectationHistory">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Circuit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Circuit">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#IndustrialCircuit">
  <rdfs:subClassOf rdf:resource="#Circuit"/>
</owl:Class>

<owl:Class rdf:about="#ResidentialCircuit">
  <rdfs:subClassOf rdf:resource="#Circuit"/>
</owl:Class>

<owl:Class rdf:about="#RuralCircuit">
  <rdfs:subClassOf rdf:resource="#Circuit"/>
</owl:Class>

<owl:Class rdf:about="#Citizen">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#Industrial1">
  <rdf:type rdf:resource="#IndustrialCircuit"/>
  <hasEconomicImpact rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1000.0</hasEconomicImpact>
  <hasAffectationHistory rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasAffectationHistory>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Residential1">
  <rdf:type rdf:resource="#ResidentialCircuit"/>
  <hasEconomicImpact rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">500.0</hasEconomicImpact>
  <hasAffectationHistory rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</hasAffectationHistory>
</owl:NamedIndividual>


</rdf:RDF>
